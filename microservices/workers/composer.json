{
    "name": "irontec/ivozprovider-workers",
    "description": "Multitenant solution for VoIP telephony providers (Async workers)",
    "authors": [
        {
            "name": "Carlos Cruz",
            "email": "carlos@irontec.com"
        },
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../library/vendor/*/*",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../library/vendor/mmoreram/gearman-bundle/*/*",
            "options": {
                "symlink": true
            }
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "IvozProvider\\": "../../library/IvozProvider"
        },
        "psr-4": {
            "": "src/",
            "Ivoz\\": "../../library/Ivoz"
        }
    },
    "autoload-dev": {
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        }
    },
    "require": {
        "h4cc/wkhtmltopdf-amd64": "0.12.3",
        "irontec/ivoz-provider-bundle": "^2.0",
        "knplabs/knp-snappy": "0.4.3",
        "mmoreram/gearman-bundle": "^4.1",
        "phpxmlrpc/phpxmlrpc": "^4.0",
        "symfony/flex": "^1.9",
        "xamin/handlebars.php": "^0.10.4"
    },
    "extra": {
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "vendor/irontec/ivoz-provider-bundle/Resources/config/parameters.yml"
        },
        "branch-alias": null
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    }
}
